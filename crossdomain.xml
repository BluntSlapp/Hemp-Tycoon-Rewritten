	<allow-access-from domain="*.adultswim.com" secure="true" to-ports="*"/>
	<allow-access-from domain="*.turner.com" to-ports="*"/>
	<allow-access-from domain="*.turner.com" secure="true" to-ports="*"/>
	<allow-access-from domain="*.amazonaws.com" to-ports="*"/>
	<allow-access-from domain="*.amazonaws.com" secure="true" to-ports="*"/>
	<allow-access-from domain="*.hemptycoongame.com" to-ports="*"/>
	<allow-access-from domain="*.hemptycoongame.com" secure="true" to-ports="*"/>
</cross-domain-policy>